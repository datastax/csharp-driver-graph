{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "src/Cassandra.DataStax.Graph/Cassandra.DataStax.Graph.csproj"
          ],
          "exclude": [ "**/bin/**", "**/obj/**" ],
          "src": ".."
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [ "api/*.yml" ]
      }
    ],
    "template": "statictoc",
    "dest": "api-docs-web",
    "globalMetadata": {
      "_appTitle": "DataStax C# Graph Extension",
      "_appFooter": "Copyright Â© DataStax",
      "_appLogoPath": "http://docs.datastax.com/en/assets/images/logo.png",
      "_disableContribution": true
    }
  }
}